{
    "name": "sauls/object-registry-bundle",
    "type": "symfony-bundle",
    "description": "Sauls symfony object registry bundle",
    "homepage": "https://saulius.vaiceliunas.lt",
    "keywords": ["object", "registry", "entity", "manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saulius Vaičeliūnas",
            "email": "vaiceliunas@inbox.lt"
        }
    ],
    "require": {
        "php": ">= 7.2",
        "sauls/collections": "^1.1",
        "symfony/http-kernel": "^3.0|^4.0",
        "symfony/config": "^3.0|^4.0",
        "symfony/yaml": "^3.0|^4.0",
        "doctrine/common": "^2.0",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Sauls\\Bundle\\ObjectRegistryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sauls\\Bundle\\ObjectRegistryBundle\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "v1.x"
        }
    }
}
